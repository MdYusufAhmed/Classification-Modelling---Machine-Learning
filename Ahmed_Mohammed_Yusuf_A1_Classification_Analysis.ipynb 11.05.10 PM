   "cell_type": "code",
   "execution_count": null,
   "id": "aeca435c",
   "metadata": {},
   "outputs": [],
   "source": [
    "# user-defined functions\n",
    "\n",
    "#########################\n",
    "# mv_flagger\n",
    "#########################\n",
    "def mv_flagger(GOT_FILE):\n",
    "    \"\"\"\n",
    "Flags all columns that have missing values with 'm-COLUMN_NAME'.\n",
    "\n",
    "PARAMETERS\n",
    "----------\n",
    "\n",
    "\n",
    "\n",
    "RETURNS\n",
    "-------\n",
    "DataFrame with missing value flags.\"\"\"\n",
    "\n",
    "\n",
    "    for col in GOT_FILE:\n",
    "\n",
    "        if GOT_FILE[col].isnull().astype(int).sum() > 0:\n",
    "            GOT_FILE['m_'+col] = GOT_FILE[col].isnull().astype(int)\n",
    "            \n",
    "    return GOT_FILE\n",
    "\n",
    "\n",
    "\n",
    "#########################\n",
    "# text_split_feature\n",
    "#########################\n",
    "def text_split_feature(col, GOT_FILE, sep=' ', new_col_name='number_of_names'):\n",
    "    \"\"\"\n",
    "Splits values in a string Series (as part of a DataFrame) and sums the number\n",
    "of resulting items. Automatically appends summed column to original DataFrame.\n",
    "\n",
    "PARAMETERS\n",
    "----------\n",
    "col          : column to split\n",
    "\n",
    "sep          : string sequence to split by, default ' '\n",
    "new_col_name : name of new column after summing split, default\n",
    "               'number_of_names'\n",
    "\"\"\"\n",
    "    \n",
    "    GOT_FILE[new_col_name] = 0\n",
    "    \n",
    "    \n",
    "    for index, val in GOT_FILE.iterrows():\n",
    "        GOT_FILE.loc[index, new_col_name] = len(GOT_FILE.loc[index, col].split(sep = ' ')) "
   ]
  },
